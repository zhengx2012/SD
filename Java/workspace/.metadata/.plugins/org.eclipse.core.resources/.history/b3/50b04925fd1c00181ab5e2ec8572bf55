package labs;

public class Fib {

	public static void main(String[] args) {
		int sum = 0, fib = 0, n1 = 0, n2 =0;
		for (int i = 0; i < 20; i++) {
			
			System.out.println(fib);
			
			if(i > 2) {
				fib = n1 + n2;
			}
			else {
				fib = fib + n1;
			}
			
			if(n1 == 0) {
				n1 = 1;
			}
			else {
				n1 = fib;
			}
			

			

			
			
		}

	}

}
